<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="www.silver.dao.IF_dao">
	<insert id="insertone" parameterType="www.silver.VO.memberVO">
		insert into SZ values(#{id},#{pass})
	</insert>
	<select id="selectone" resultType="www.silver.VO.memberVO">
		select * from SZ where id = #{id}
	</select>
</mapper>